{
  "languageserver": {
    "go": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod"
      ],
      "trace.server": "verbose",
      "filetypes": [
        "go"
      ]
    }
  },
  "eslint.autoFixOnSave": true,
  "eslint.format.enable": true,
  "solargraph.diagnostics": true,
  "solargraph.autoformat": true,
  "solargraph.formatting": true,
  "java.home": "~/.asdf/installs/java/zulu-11.31.11",
  "diagnostic.enable": true,
  "diagnostic.messageTarget": "echo",
  "diagnostic.checkCurrentLine": true,
  "diagnostic.displayByAle": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "c",
    "go",
    "ruby",
    "python",
    "markdown",
    "javascript",
    "typescript",
    "json",
    "jsonc"
  ],
  "list.normalMappings": {
    "<C-c>": "do:exit"
  },
  "list.insertMappings": {
    "<C-c>": "do:exit"
  },
  "typescript.format.semicolons": "insert",
  "cSpell.userWords": [
    "adobo"
  ],
  "cSpell.enabledLanguageIds": [
    "asciidoc",
    "c",
    "cpp",
    "csharp",
    "css",
    "git-commit",
    "go",
    "handlebars",
    "haskell",
    "html",
    "jade",
    "java",
    "javascriptreact",
    "json",
    "jsonc",
    "latex",
    "less",
    "markdown",
    "php",
    "plaintext",
    "pug",
    "python",
    "restructuredtext",
    "rust",
    "scala",
    "scss",
    "text",
    "typescriptreact",
    "vim",
    "yml"
  ]
}
